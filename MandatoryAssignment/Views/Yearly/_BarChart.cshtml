@using System.Globalization
<div id="graph"></div>
<script>
    new Morris.Bar({
        element: 'graph',
        data: [
            { month: '2015-01', value: @Model.January.ToString(new CultureInfo("en-US")) },
            { month: '2015-02', value: @Model.February.ToString(new CultureInfo("en-US"))  },
            { month: '2015-03', value: @Model.March.ToString(new CultureInfo("en-US"))  },
            { month: '2015-04', value: @Model.April.ToString(new CultureInfo("en-US"))  },
            { month: '2015-05', value: @Model.May.ToString(new CultureInfo("en-US"))  },
            { month: '2015-06', value: @Model.June.ToString(new CultureInfo("en-US"))  },
            { month: '2015-07', value: @Model.July.ToString(new CultureInfo("en-US"))  },
            { month: '2015-08', value: @Model.August.ToString(new CultureInfo("en-US"))  },
            { month: '2015-09', value: @Model.September.ToString(new CultureInfo("en-US"))  },
            { month: '2015-10', value: @Model.October.ToString(new CultureInfo("en-US"))  },
            { month: '2015-11', value: @Model.November.ToString(new CultureInfo("en-US"))  },
            { month: '2015-12', value: @Model.December.ToString(new CultureInfo("en-US"))  },
        ],
        xLabels: 'month',
        ymin: 'auto',
        xkey: 'month',
        ykeys: ['value'],
        labels: ['Average'],
        xLabelAngle: 45,
        pointFillColors: ['#f0ad4e'],
        pointSize: 5,
        hideHover: 'auto',
        resize: true
    });
</script>